{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "printWidth": 130,
        "semi": false,
        "singleQuote": true
      }
    ],
    "react/prop-types": 0,
    "import/no-unresolved": 0
  },
  "globals": {
    "chrome": true,
    "browser": true
  }
}
